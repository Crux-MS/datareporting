		formatString: 0
		displayFolder: Flags
		lineageTag: c9d01716-a689-4e2d-a63b-3a9613b38a20

	measure 'KP Reporting Month Flag' =
			VAR _selectedDate = CALCULATE(MAX('Dim Extract Date'[Extract Date]), CROSSFILTER('Dim Extract Date'[Extract Date], 'Fact Pay'[ReportingMonthStart], Both))
			VAR _startDate = EOMONTH(_selectedDate, -2)+1
			VAR _endDate = EOMONTH(_selectedDate, -1)
			VAR _selectedStartDate = Min('Dim Paid Date'[PaidDate])
			VAR _logic = IF(_selectedStartDate >= _startDate && _selectedStartDate <= _endDate,
			    1,
			    0)
			RETURN _logic
		formatString: 0
		displayFolder: Flags
