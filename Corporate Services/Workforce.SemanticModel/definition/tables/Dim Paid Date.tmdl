table 'Dim Paid Date'
	lineageTag: 3cb10c54-5c14-4fd5-86f9-c78f607783e8

	measure 'KP Reporting Year Flag' =
			VAR _selectedDate = MAX('Dim Extract Date'[Extract Date])
			VAR _startDate = EOMONTH(_selectedDate, -12)+1
			VAR _endDate = EOMONTH(_selectedDate, -1)
			VAR _selectedStartDate = Min('Dim Paid Date'[PaidDate])
			VAR _logic = IF(_selectedStartDate >= _startDate && _selectedStartDate <= _endDate,
			    1,
			    0)
			RETURN _logic
		formatString: 0
		displayFolder: Flags
		lineageTag: 87183d63-acaa-4503-be62-ea833ab55eb0

	measure 'KP Reporting 6-mth Flag' =
			VAR _selectedDate = MAX('Dim Extract Date'[Extract Date])
			VAR _startDate = EOMONTH(_selectedDate, -7)+1
